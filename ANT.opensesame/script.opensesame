define inline_script block_x_starts
	___run__
	# Import the widgets library
	from libopensesame import widgets
	
	# Create a form
	form = widgets.form(self.experiment, cols=[1], rows=[1],
		margins=(250,500,250,500), spacing=0)
	
	if 'block' in locals():
		pass
	else:
		block = 1
		
	if block == 1:
		rest = 'Das war die U+00DCbungsphase.'
	else:
		rest = ''
	# Create four widgets
	text = widgets.label(form, text=rest + 'Gleich startet der %d. Block. Wenn Sie bereit sind, drU+00FCcken Sie die LEERTASTE, um zu beginnen.' % block)
	
	# Add the widgets to the form. The position in the form is indicated as a
	# (column, row) tuple.
	form.set_widget(text, (0,0))
	
	block = block + 1
	exp.set('block', block)
	
	form.render()
	__end__
	set _prepare ""
	set description "Executes Python code"

